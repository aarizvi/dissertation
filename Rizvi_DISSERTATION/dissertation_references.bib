	Mesh = {Bias; Chromosome Mapping; Disease; Genetic Linkage; Genetic Markers; Genetic Predisposition to Disease; Genetic Variation; Humans; Meta-Analysis as Topic; Odds Ratio; Polymorphism, Genetic; Regression Analysis; Reproducibility of Results},
	Month = {Nov},

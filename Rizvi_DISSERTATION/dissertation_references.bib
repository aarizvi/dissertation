	Month = {Dec},
	Pages = {4523-31},
	Pmid = {11082045},
	Pst = {ppublish},
	Title = {Human Cdc5, a regulator of mitotic entry, can act as a site-specific DNA binding protein},
	Volume = {113 Pt 24},

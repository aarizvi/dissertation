	Doi = {10.1016/j.bbmt.2015.08.037},
	Journal = {Biol Blood Marrow Transplant},
	Journal-Full = {Biology of blood and marrow transplantation : journal of the American Society for Blood and Marrow Transplantation},
	Keywords = {Costimulation; GVHD; Gene polymorphism; Hematopoietic stem cell transplantation; Infection; Outcomes},
	Mesh = {Adolescent; Adult; Aged; Antigens, CD; B-Lymphocytes; Case-Control Studies; Child; Child, Preschool; Disease-Free Survival; Female; HLA Antigens; Hematologic Neoplasms; Hematopoietic Stem Cell Transplantation; Homozygote; Humans; Infant; Male; Middle Aged; OX40 Ligand; Oncogene Proteins v-myb; Polymorphism, Single Nucleotide; Survival Rate},

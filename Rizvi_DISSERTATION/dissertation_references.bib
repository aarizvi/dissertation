	Mesh = {Animals; Basic Helix-Loop-Helix Transcription Factors; Cell Differentiation; GTP-Binding Proteins; Hematopoiesis; Humans; Membrane Proteins; Mice; Oncogenes; Precursor T-Cell Lymphoblastic Leukemia-Lymphoma; Proto-Oncogene Proteins; Receptors, Antigen, T-Cell; T-Cell Acute Lymphocytic Leukemia Protein 1; Tumor Suppressor Proteins},
	Month = {09},

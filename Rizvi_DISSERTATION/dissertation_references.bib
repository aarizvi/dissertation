	Keywords = {Acute leukemia; Adjudication; Allogeneic HCT; Cause-specific mortality},
	Mesh = {Adult; Aged; Algorithms; Allografts; Female; Follow-Up Studies; Hematologic Neoplasms; Hematopoietic Stem Cell Transplantation; Humans; Male; Middle Aged; Unrelated Donors},
